{
  "name": "todo-app",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "antd": "^5.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "15.0.0",
    "react-scripts": "5.0.1",
    "relay-runtime": "15.0.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "build": "relay-compiler && react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx .",
    "relay": "relay-compiler",
    "schema:local": "get-graphql-schema http://localhost:4000/graphql > src/schema.graphql",
